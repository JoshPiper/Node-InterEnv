{
  "name": "@doctor_internet/interenv",
  "version": "2.2.0",
  "description": "A simple environment processing system.",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "coverage": "nyc --reporter=lcov mocha -r ts-node/register test/**/*.spec.ts",
    "readme": "common-readme > README.md",
    "release": "npx semantic-release",
    "dev": "npx tsc --watch",
    "prod": "npx tsc",
    "clean": "rm index.js index.d.ts 2>/dev/null || true",
    "build": "npm run clean && npm run prod",
    "prepack": "npm run build"
  },
  "keywords": [],
  "author": "Joshua Piper",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoshPiper/Node-InterEnv.git"
  },
  "dependencies": {
    "dotenv-expand": "^5.1.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.5",
    "@types/sinon": "^10.0.17",
    "chai": "^4.3.8",
    "common-readme": "^1.1.0",
    "mocha": "^6.2.3",
    "nyc": "^15.1.0",
    "semantic-release": "^22.0.1",
    "sinon": "^16.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src/index.js",
    "src/index.d.ts",
    "src/errors/EnvironmentVariableNotFoundException.js",
    "src/errors/EnvironmentVariableNotFoundException.d.ts",
    "src/errors/UnableToConvertBooleanException.js",
    "src/errors/UnableToConvertBooleanException.d.ts",
    "src/errors/UnableToConvertNumberException.js",
    "src/errors/UnableToConvertNumberException.d.ts",
    "src/types.d.ts"
  ]
